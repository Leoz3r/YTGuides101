#include <stdio.h>
#include <stdbool.h>
// must include when using boolean 

int main(void) {

  // logical operators == && (AND) checks if two conditions are true

  float temp = 25;
  bool sunny = true;


  if(temp >= 0 && temp <= 30 && sunny){
    printf("\nThe weather is good!");
  }
  else {
    printf("\nThe weather is bad!");
  }

  return 0;
}